{
  "name": "11ty-portfolio",
  "version": "1.1.0",
  "description": "My personal website built with the static site generator Eleventy",
  "main": ".eleventy.js",
  "jest": {
    "verbose": true
  },
  "scripts": {
    "watch:sass": "sass --watch src/_includes/sass:src/_includes/css",
    "build:sass": "sass src/_includes/sass:src/_includes/css",
    "watch:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "build:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "dev": "npm-run-all watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* @11ty/eleventy",
    "lint": "stylelint src/_includes/sass/*.scss",
    "test:scripts": "jest"
  },
  "keywords": [
    "static site",
    "Eleventy",
    "11ty"
  ],
  "author": {
    "name": "Tanner Dolby",
    "url": "https://github.com/tannerdolby"
  },
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@types/jest": "^26.0.24",
    "capture-website": "^1.4.0",
    "cheerio": "^1.0.0-rc.10",
    "clean-css": "^4.2.3",
    "eleventy-plugin-metagen": "^1.7.7",
    "eleventy-plugin-sharp-respimg": "^1.3.4",
    "eleventy-plugin-social-img": "^1.2.6",
    "markdown-it": "^12.2.0",
    "npm-run-all": "^4.1.5",
    "nunjucks-date-filter": "^0.1.1",
    "sass": "^1.39.0",
    "slugify": "^1.6.0",
    "speedlify-score": "^2.0.2",
    "terser": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerdolby/tannerdolby.com.git"
  },
  "bugs": {
    "url": "https://github.com/tannerdolby/tannerdolby.com/issues"
  },
  "homepage": "https://github.com/tannerdolby/tannerdolby.com/#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "grunt": "^1.4.1",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  }
}
